{
  "name": "dataservices",
  "version": "1.0.0",
  "description": "DataServices prototype",
  "main": "app.js",
  "scripts": {
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "clean": "gulp clean",
    "live": "gulp live",
    "prod": "gulp production rebuild",
    "start": "node app.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "jade": "^1.11.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-immutable-proptypes": "^1.6.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "0.14.7"
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": 0,
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": 1,
      "no-console": 1
    }
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "browserify-incremental": "^3.1.1",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.0",
    "gulp-streamify": "^1.0.2",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "merge-stream": "^1.0.0",
    "sassify": "^0.9.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "author": "JitCoder"
}
